{
  "name": "zombie-api",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf ./build && npm run copy-files && tsc",
    "start": "npm run build && node build/server.js",
    "start:dev": "nodemon",
    "typeorm:cli": "ts-node ./node_modules/typeorm/cli -f ./src/common/mongo/ormconfig.ts",
    "migration:run": "npm run typeorm migration:run",
    "migration:generate": "npm run typeorm migration:generate -n",
    "migration:blank": "npm run typeorm migration:create -n",
    "copy-files": "copyfiles swagger.yaml build",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "npm run lint -- --fix",
    "prettier": "prettier . --ext .ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LarryErbs/zombie-api.git"
  },
  "keywords": [],
  "author": "",
